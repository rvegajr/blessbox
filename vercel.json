{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "development": true
    }
  },
  "github": {
    "deploymentEnabled": true,
    "autoAlias": true
  },
  "functions": {
    "src/pages/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production",
    "PUBLIC_APP_URL": "https://blessbox.vercel.app"
  },
  "build": {
    "env": {
      "NODE_ENV": "development", 
      "PUBLIC_APP_URL": "https://blessbox-git-development.vercel.app"
    }
  }
}